{% extends './layout.twig' %}

{% block head %}
<title>Add response</title>
<script src="/js/addresponse.js"></script>
{% endblock %}


{% block body %}
<main>

  <form method="post">
    <h2>New response</h2>
    <label for="name" class="label">Intent name
      <input type="text" name="name" id="name" class="input"/>
    </label>
    <label for="beauty" class="label">Intent beauty name
      <input type="text" name="beauty" id="beauty" class="input"/>
    </label>
    <label for="type" class="label">Intent type
      <input type="text" name="type" id="type" class="input"/>
    </label>

    <button class="btn">Save</button>
  </form>

</main>
{% endblock %}
